exports.seed = (knex, Promise) ->
  uid = 0
  ins = (id, name="") ->
    slug = name.toLowerCase().replace(/&/g, "").replace /[,\s]+/g, "-"
    values =
      id: ++uid
      slug: "#{slug}-#{uid}"
      name: name
    (knex "locations").insert values

